{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/auth/about", "app/auth/pricing", "components/auth.config.js", "components/provider/_app.tsx", "app/api/auth/[...nextauth]/authOptions", "app/api/razorpay/razorpay.js", "app/(public)/_components/Design.jsx", ".next/types/app/api/OpenAi/route.tsx"],
  "exclude": ["node_modules"]
}
